---
import '../styles/global.css';

import SEO from "./SEO.astro";

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<SEO />
		<title>{title}</title>
		<link rel="icon" type="image/svg+xml" href="/logo.svg" />
	</head>
	<body>
		<slot />
	</body>
</html>
